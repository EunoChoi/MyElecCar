{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
      "prettier",
      "airbnb",
      "airbnb/hooks",
      "plugin:prettier/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": ["./tsconfig.json"]
    },
    "plugins": [
        "react",
        "@typescript-eslint"
    ],
    "settings": {
        "import/resolver": {
            "node": {
                "extensions": [".js", ".jsx", ".ts", ".tsx"],
                "modeulDirectory": ["node_modules", "src/"]
            }
        }
    },
    "parser": "@babel/eslint-parser",
    "rules": {
        "linebreak-style": 0,
        "import/prefer-default-export": 0,
        "prettier/prettier": 0,
        "import/extenstions": 0,
        "no-use-before-define": 0,
        "import/no-unresolved": 0,
        "import/no-extraneous-dependencies": 0,
        "no-shadow": 0,
        "react/prop-types": 0,
        "react/jsx-filename-extension": [
            2,
            { "extensions": [".js", ".jsx", ".ts", ".tsx"] }
        ],
        "jsx-ally/no-noninteractive-element-interations": 0
    }
}
